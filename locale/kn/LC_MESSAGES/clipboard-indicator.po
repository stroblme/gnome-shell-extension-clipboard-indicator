#: extension.js:102
msgid "Text will be here"
msgstr "ಪಠ್ಯ ಇಲ್ಲಿ ಇರುತ್ತದೆ"

#: extension.js:171
msgid "Type here to search..."
msgstr "ಹುಡುಕಲು ಇಲ್ಲಿ ಟೈಪ್ ಮಾಡಿ..."

#: extension.js:240 prefs.js:163
msgid "Private mode"
msgstr "ಖಾಸಗಿ ಮೋಡ್"

#: extension.js:254 prefs.js:165
msgid "Clear history"
msgstr "ಇತಿಹಾಸವನ್ನು ಅಳಿಸಿ"

#: extension.js:266
msgid "Settings"
msgstr "ಸೆಟ್ಟಿಂಗ್‌ಗಳು"

#: extension.js:289
msgid "Clipboard is empty"
msgstr "ಕ್ಲಿಪ್‌ಬೋರ್ಡ್ ಖಾಲಿಯಾಗಿದೆ"

#: extension.js:552
msgid "Clear all?"
msgstr "ಎಲ್ಲಾ ಅಳಿಸಬಹುದೇ?"

#: extension.js:553
msgid "Are you sure you want to delete all clipboard items?"
msgstr "ನೀವು ಎಲ್ಲಾ ಕ್ಲಿಪ್‌ಬೋರ್ಡ್ ಐಟಂಗಳನ್ನು ಅಳಿಸಲು ಖಚಿತವೇ?"

#: extension.js:554
msgid "This operation cannot be undone."
msgstr "ಈ ಕಾರ್ಯವನ್ನು ಹಿಂತೆಗೆದುಕೊಳ್ಳಲು ಸಾಧ್ಯವಿಲ್ಲ."

#: extension.js:556
msgid "Clear"
msgstr "ಅಳಿಸಿ"

#: extension.js:556 extension.js:714
msgid "Cancel"
msgstr "ರದ್ದುಗೊಳಿಸಿ"

#: extension.js:569
msgid "Clipboard history cleared"
msgstr "ಕ್ಲಿಪ್‌ಬೋರ್ಡ್ ಇತಿಹಾಸ ಅಳಿಸಲಾಗಿದೆ"

#: extension.js:713
msgid "Copied to clipboard"
msgstr "ಕ್ಲಿಪ್‌ಬೋರ್ಡ್‌ಗೆ ನಕಲಿಸಲಾಗಿದೆ"

#: prefs.js:28
msgid "History Size"
msgstr "ಇತಿಹಾಸದ ಗಾತ್ರ"

#: prefs.js:37
msgid "Preview Size (characters)"
msgstr "ಪೂರ್ವವೀಕ್ಷಣಾ ಗಾತ್ರ (ಅಕ್ಷರಗಳು)"

#: prefs.js:46
msgid "Max cache file size (MB)"
msgstr "ಗರಿಷ್ಠ ಕಾಚೆ ಫೈಲ್ ಗಾತ್ರ (MB)"

#: prefs.js:55
msgid "Number of characters in top bar"
msgstr "ಟಾಪ್ ಬಾರ್‌ನ ಅಕ್ಷರಗಳ ಸಂಖ್ಯೆ"

#: prefs.js:64
msgid "What to show in top bar"
msgstr "ಟಾಪ್ ಬಾರ್‌ನಲ್ಲಿ ಏನು ತೋರಿಸಬೇಕು"

#: prefs.js:69
msgid "Remove down arrow in top bar"
msgstr "ಟಾಪ್ ಬಾರ್‌ನ ಕೆಳಗಿನ ಬಾಣವನ್ನು ತೆಗೆದುಹಾಕಿ"

#: prefs.js:73
msgid "Cache only pinned items"
msgstr "ಪಿನ್ ಮಾಡಿರುವ ಐಟಂಗಳನ್ನು ಮಾತ್ರ ಕಾಶೆ ಮಾಡಿ"

#: prefs.js:77
msgid "Show notification on copy"
msgstr "ನಕಲಿಸಿದಾಗ ಅಧಿಸೂಚನೆ ತೋರಿಸಿ"

#: prefs.js:81
msgid "Show confirmation on Clear History"
msgstr "ಇತಿಹಾಸವನ್ನು ಅಳಿಸುವಾಗ ದೃಢೀಕರಣ ತೋರಿಸಿ"

#: prefs.js:85
msgid "Remove whitespace around text"
msgstr "ಪಠ್ಯದ ಸುತ್ತಲಿನ ಖಾಲಿ ಸ್ಥಳವನ್ನು ತೆಗೆದುಹಾಕಿ"

#: prefs.js:89
msgid "Move item to the top after selection"
msgstr "ಆಯ್ಕೆ ಮಾಡಿದ ನಂತರ ಐಟಂವನ್ನು ಮೇಲಕ್ಕೆ ಸರಿಸಿ"

#: prefs.js:93
msgid "Keep selected entry after Clear History"
msgstr "ಇತಿಹಾಸ ಅಳಿಸಿದ ನಂತರ ಆಯ್ಕೆ ಮಾಡಿದ ಎಂಟ್ರಿಯನ್ನು ಉಳಿಸಿ"

#: prefs.js:97
msgid "Show paste buttons"
msgstr "ಪೇಸ್ಟ್ ಬಟನ್‌ಗಳನ್ನು ತೋರಿಸಿ"

#: prefs.js:98
msgid "Adds a paste button to each entry that lets you paste it directly"
msgstr "ಪ್ರತ್ಯೇಕ ಎಂಟ್ರಿಗೆ ನೇರವಾಗಿ ಪೇಸ್ಟ್ ಮಾಡಲು ಪೇಸ್ಟ್ ಬಟನ್ ಸೇರಿಸುತ್ತದೆ"

#: prefs.js:102
msgid "Place the pinned section on the bottom"
msgstr "ಪಿನ್ ಮಾಡಿರುವ ವಿಭಾಗವನ್ನು ಕೆಳಭಾಗದಲ್ಲಿ ಇರಿಸಿ"

#: prefs.js:103
msgid "Requires restarting the extension"
msgstr "ವಿಸ್ತರಣೆಯನ್ನು ಮರುಪ್ರಾರಂಭಿಸಲು ಅಗತ್ಯವಿದೆ"

#: prefs.js:106
msgid "UI"
msgstr "ಬಳಕೆದಾರ ಇಂಟರ್‌ಫೇಸ್"

#: prefs.js:107
msgid "Limits"
msgstr "ಮಿತಿಗಳು"

#: prefs.js:108
msgid "Topbar"
msgstr "ಟಾಪ್‌ಬಾರ್"

#: prefs.js:109
msgid "Notifications"
msgstr "ಅಧಿಸೂಚನೆಗಳು"

#: prefs.js:110
msgid "Shortcuts"
msgstr "ಶಾರ್ಟ್‌ಕಟ್‌ಗಳು"

#: prefs.js:151
msgid "Icon"
msgstr "ಚಿಹ್ನೆ"

#: prefs.js:152
msgid "Clipboard Content"
msgstr "ಕ್ಲಿಪ್‌ಬೋರ್ಡ್ ವಿಷಯ"

#: prefs.js:153
msgid "Both"
msgstr "ಎರಡೂ"

#: prefs.js:154
msgid "Neither"
msgstr "ಯಾವುದೂ ಇಲ್ಲ"

#: prefs.js:165
msgid "Toggle the menu"
msgstr "ಮೆನು ಪರಿವರ್ತನೆ ಮಾಡಿ"

#: prefs.js:167
msgid "Previous entry"
msgstr "ಹಿಂದಿನ ಎಂಟ್ರಿ"

#: prefs.js:168
msgid "Next entry"
msgstr "ಮುಂದಿನ ಎಂಟ್ರಿ"

#: prefs.js:173
msgid "Enable shortcuts"
msgstr "ಶಾರ್ಟ್‌ಕಟ್‌ಗಳನ್ನು ಸಕ್ರಿಯಗೊಳಿಸಿ"

#: prefs.js:198
msgid "Disabled"
msgstr "ನಿಷ್ಕ್ರಿಯಗೊಳಿಸಲಾಗಿದೆ"

#: prefs.js:207
msgid "Enter shortcut"
msgstr "ಶಾರ್ಟ್‌ಕಟ್ ನಮೂದಿಸಿ"
